                <MuxGroup count="0">
                    <Signal name="Voltage_Input_L1_Calib" offset="0" length="16" endianess="little">
                        <Notes>Voltage offset at the converter input, phase 1</Notes>
                        <Consumer>
                            <NodeRef id="1"/>
