                        <Value type="signed" unit="ADC Counts"/>
                    </Signal>
                </MuxGroup>
                <MuxGroup count="1">
                    <Signal name="Voltage_Input_L2_Calib" offset="0" length="16" endianess="little">
                        <Notes>Voltage offset at the converter input, phase 2</Notes>
