        <Message id="0xD0044" length="6" name="VG11_Voltage_Setpoint_Control" format="extended">
            <Notes>This message is used to set the voltage setpoint</Notes>
            <Producer>
                <NodeRef id="0"/>
            </Producer>

            <Signal name="Voltage_Setpoint" offset="0" length="16" endianess="little">
                <Notes>Voltage setpoint. For Grid Following mode, this selects the DC output 
                voltage setpoint.</Notes>
                <Consumer>
                    <NodeRef id="13"/>
                </Consumer>
                <Value type="unsigned" slope="0.1" min="0" max="1000" unit="V"/>
            </Signal>
        </Message>

        <Message id="0xD0043" length="6" name="VG11_Current_Setpoint_Control" format="extended">
            <Notes>This message is used to set the current setpoint</Notes>
            <Producer>
                <NodeRef id="0"/>
            </Producer>

            <Signal name="Current_Setpoint" offset="0" length="16" endianess="little">
                <Notes>Current setpoint. For Grid Following mode, this selects the DC output 
                current setpoint.</Notes>
                <Consumer>
                    <NodeRef id="13"/>
                </Consumer>
                <Value type="signed" slope="0.1" min="-35" max="35" unit="A"/>
            </Signal>
        </Message>

        <Message id="0xC8006" length="3" name="VG11_AC_Faults" interval="100" format="extended">
            <Notes>Fault bitfield</Notes>
            <Producer>
                <NodeRef id="12"/>
            </Producer>

            <Signal name="Protection_trip_internal" offset="0" length="1" endianess="little">
                <Notes>This flagg is asserted if the interlock is open due to an internal fault condition (self-protection)</Notes>
                <LabelSet>
                    <Label name="fault" value="1" type="error"/>
                </LabelSet>
            </Signal>
            <Signal name="Protection_trip_external" offset="1" length="1" endianess="little">
                <Notes>This flag is asserted if the interlock is open due to an external condition received in the module.</Notes>
                <LabelSet>
                    <Label name="fault" value="1" type="error"/>
                </LabelSet>
            </Signal>
            <Signal name="Mains_Voltage" offset="2" length="1" endianess="little">
                <Notes>Mains voltage out of tolerance</Notes>
                <LabelSet>
                    <Label name="fault" value="1" type="error"/>
                </LabelSet>
            </Signal>
            <Signal name="Mains_Frequency" offset="3" length="1" endianess="little">
                <Notes>Mains frequency out of tolerance</Notes>
                <LabelSet>
                    <Label name="fault" value="1" type="error"/>
                </LabelSet>
            </Signal>
            <Signal name="Precharge" offset="4" length="1" endianess="little">
