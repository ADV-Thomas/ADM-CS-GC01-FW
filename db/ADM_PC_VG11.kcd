		<Message id="0xD8007" length="3" name="VG11_Status" interval="100" format="extended">
            <Notes>Status bitfield</Notes>
            <Producer>
                <NodeRef id="13"/>
            </Producer>

            <Signal name="Idle" offset="0" length="1" endianess="little">
                <Notes>Indicates that the converter is idle (i.e: not operating).</Notes>
            </Signal>
            <Signal name="Relay_Connected" offset="1" length="1" endianess="little">
                <Notes>Indicates that the mains relay is connected.</Notes>
            </Signal>
            <Signal name="Operating" offset="2" length="1" endianess="little">
                <Notes>Converter is running and mosfets are switching.</Notes>
            </Signal>
            <Signal name="Grid_Following" offset="3" length="1" endianess="little">
                <Notes>Indicates if the converter is operating in "Grid Following" mode
                (i.e: attached to an existing grid)</Notes>
            </Signal>
            <Signal name="PWM" offset="5" length="1" endianess="little">
                <Notes>The converter is running in PWM mode (customers must not use
                this operation mode).</Notes>
            </Signal>
            <Signal name="PFC_Enabled" offset="6" length="1" endianess="little">
                <Notes>PFC enabled</Notes>
            </Signal>
            <Signal name="AC_DAB_Enabled" offset="7" length="1" endianess="little">
                <Notes>AC side DAB is enabled</Notes>
            </Signal>
            <Signal name="DC_DAB_Enabled" offset="8" length="1" endianess="little">
                <Notes>DC side DAB is enabled</Notes>
            </Signal>
            <Signal name="Buck_Enabled" offset="9" length="1" endianess="little">
                <Notes>Buck is enabled</Notes>
            </Signal>
            <Signal name="State" offset="16" length="8" endianess="little">
                <Notes>For debugging and internal use. User can ignore it.</Notes>
            </Signal>
        </Message>

        <Message id="0xD0007" length="2" name="VG11_Mode_Control" format="extended">
