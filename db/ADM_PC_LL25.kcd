        <Message id="0x3800B" length="2" name="_LLC_Ground_Fault_Raw" interval="100" format="extended">
            <Notes>Ground fault measurement</Notes>
            <Producer>
                <NodeRef id="3"/>
            </Producer>

            <Signal name="InsulationResistance_Raw" offset="0" length="16">
                <Notes>The insulation resistance between output terminals and ground</Notes>
                <Value type="signed"/>
            </Signal>
        </Message>

