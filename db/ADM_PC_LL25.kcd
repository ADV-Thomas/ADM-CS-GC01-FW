            <Signal name="Gnd_monitor_enabled" offset="33" length="1" endianess="little">
                <Notes>
                    True if the Ground insulation monitor is enabled.
                </Notes>
            </Signal>
            <Signal name="Gnd_test_enabled" offset="34" length="1" endianess="little">
                <Notes>
                    True if the Ground test circuit is enabled.
                </Notes>
            </Signal>
