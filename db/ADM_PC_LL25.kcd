                <Value type='signed' slope="0.1" unit="V"/>
            </Signal>
            <Signal name="Voltage_Out" offset="32" length="16">
                <Notes>Voltage measured at the converter output</Notes>
                <Value type='signed' slope="0.1" unit="V"/>
            </Signal>
        </Message>

